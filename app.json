{
  "expo": {
    "name": "challenge-krug",
    "slug": "challenge-krug",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "challenge-krug",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.krugalan.challengekrug"
    },
    "android": {
      "package": "com.krugalan.challengekrug",
      "versionCode": 1
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/inter/Inter_900Black.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}